{
	"manifest_version": 2,
	"name": "chatwork-my-extensions",
	"version": "2018.07.31",
	"description": "より効率よくチャットワークを扱うための拡張機能",
	"author": "IwakamiYuki",
	"icons": {
		"16" : "images/icon16.png",
		"48" : "images/icon48.png",
		"128" : "images/icon128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.chatwork.com/*",
				"https://kcw.kddi.ne.jp/*"
			],
			"js": [
				"jquery.js",
				"migemo.js",
				"common.js"
			],
			"css": [
				"style.css"
			]
		}
	],
	"permissions": [
	],
	"background": {
	}
}
